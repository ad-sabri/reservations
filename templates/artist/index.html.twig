{% extends 'base.html.twig' %} 

{% block title %}Hello ArtistController!{% endblock %}

{% block body %}
    <h1>Liste des Artistes</h1>
    <table>
        <thead>
            <tr>
                <th>Firstname</th>
                <th>Lastname</th>
            </tr>
        </thead>
        <tbody>
        {% for artist in artists %}
            <tr>
                <td>{{artist.firstname}}</td>
                <td>{{artist.lastname}}</td>
            </tr>
        {% endfor %}    
        </tbody>
    </table>
{% endblock %}
